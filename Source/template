\documentclass[12pt,border=2pt,multi]{standalone}
\newenvironment{yuriemathform}{\ignorespaces}{\ignorespacesafterend}
\standaloneenv{yuriemathform}
\newbox\YurieMathFormbox
\newcommand{\YurieMathForm}[1]{
    \begin{yuriemathform}
        \setbox\YurieMathFormbox\hbox{\(\displaystyle #1 \)}%
        \typeout{YurieMathFormSizeBegin,\the\dp\YurieMathFormbox,\the\ht\YurieMathFormbox,\the\wd\YurieMathFormbox,YurieMathFormSizeEnd}%
        \unhbox\YurieMathFormbox%
    \end{yuriemathform}
}
