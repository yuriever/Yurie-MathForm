\documentclass[12pt,border=1pt,multi]{standalone}
\newenvironment{yuriemathform}{\ignorespaces}{\ignorespacesafterend}
\standaloneenv{yuriemathform}
\newbox\YurieMathFormbox
\newcommand{\YurieMathForm}[1]{
    \begin{yuriemathform}
        \setbox\YurieMathFormbox\hbox{\(\displaystyle #1 \)}%
        \unhbox\YurieMathFormbox%
    \end{yuriemathform}
}
